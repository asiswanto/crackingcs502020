#include<stdio.h>
#include<cs50.h>
#include<math.h>

int main(void)
{
    float dollars; //use float u dip shit, dont use int
    int c;
    do
    {
        dollars = get_float("Change owed: ");
    }
    while (dollars < 0);
    int cents = round(dollars * 100);
    int coins = 0;
    while (cents >= 25)
    {
        coins = coins + 1;
        cents = cents - 25;
    }
    while (cents >= 10)
    {
        coins = coins + 1;
        cents = cents - 10;
    }
    while (cents >= 5)
    {
        coins = coins + 1;
        cents = cents - 5;
    }  
    while (cents >= 1)
    {
        coins = coins + 1;
        cents = cents - 1;
    }
    printf("%i\n", coins);
}
